{
  "tasks": {
    "start": "deno run -A npm:vite",
    "build": "vite build",
    "preview": "deno serve -A _fresh/server.js"
  },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "imports": {
    "$std/": "https://deno.land/std@0.224.0/",
    "@preact-icons/lu": "jsr:@preact-icons/lu@^1.0.13",
    "esbuild": "npm:esbuild@^0.27.2",
    "preact": "npm:preact@^10.28.2",
    "preact/hooks": "npm:preact@^10.28.2/hooks",
    "preact/jsx-runtime": "npm:preact@^10.28.2/jsx-runtime",
    "preact/debug": "npm:preact@^10.28.2/debug",
    "fresh": "jsr:@fresh/core@^2.2.0",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.8",
    "rollup": "npm:rollup@^4.55.1",
    "vite": "npm:vite@^7.3.1",
    "@preact/signals": "npm:@preact/signals@^2.5.1"
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"]
}
